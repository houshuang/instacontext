{
  "name": "InstaContext",
  "description": "Right-click context menu option to save a link to Instapaper",
  "version": "0.1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/favicon.ico",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "contextMenus",
    "https://www.instapaper.com/api/add"
  ]
}